	<artifactId>pdok-ngr-dar</artifactId>
	<packaging>dar</packaging>
	<build>
		<plugins>
			<plugin>
				<groupId>com.xebialabs.deployit</groupId>
				<artifactId>maven-deployit-plugin</artifactId>
				<version>4.5.2</version>
				<extensions>true</extensions>
				<executions>
					<execution>
						<id>deployit-generate-package</id>
						<phase>package</phase>
						<goals>
							<goal>generate-deployment-package</goal>
						</goals>
					</execution>
					<execution>
						<id>deployit-plugin-deploy</id>
						<phase>deploy</phase>
						<goals>
							<goal>deploy</goal>
						</goals>
					</execution>
				</executions>
				<configuration>
					<username>${deployit.username}</username>
					<password>${deployit.password}</password>
					<serverAddress>${deployit.server}</serverAddress>
					<deletePreviouslyDeployedDar>true</deletePreviouslyDeployedDar>
					<generateDeployedOnUpgrade>true</generateDeployedOnUpgrade>
					<applicationName>pdok/${project.artifactId}</applicationName>
					<environmentId>Environments/pdok-int/environment/pdok-ngr_int</environmentId>
			<deployables>
				<deployable>
                                        <name>DNS-ngr-{{application}}-{{werkomgeving}}</name>
                                       	<type>DNS.dnsAliasSpec</type>
                        	                <dnsAlias>
                                                       	<list>
                                                               	<value>{{dns.ngr}}</value>
                                                        </list>
                                                </dnsAlias>
                                </deployable>
				<deployable>
                            <name>singleinstance</name>
                            <type>tomcat.SingleInstanceSpec</type>
                            <activeNode>{{activenode}}</activeNode>
                        </deployable>
                        <deployable>
                            <name>javaProps</name>
                            <type>tomcat.JavaPropsSpec</type>
                            <properties>
                                <map>
                                    <http--proxyHost>www-proxy.cs.kadaster.nl</http--proxyHost>
                                    <http--proxyPort>8082</http--proxyPort>
                                    <http--nonProxyHosts>localhost|127.0.0.1|*nationaalgeoregister.nl|pdok-*|10.*|*.CS.kadaster.nl|*.FT4.kadaster.nl|*.FTO.kadaster.nl|*.IN.kadaster.nl|*.SO.kadaster.nl</http--nonProxyHosts>
                                    <https--proxyHost>www-proxy.cs.kadaster.nl</https--proxyHost>
                                    <https--proxyPort>8082</https--proxyPort>
                                    <https--nonProxyHosts>localhost|127.0.0.1|*nationaalgeoregister.nl|pdok-*|10.*|*.CS.kadaster.nl|*.FT4.kadaster.nl|*.FTO.kadaster.nl|*.IN.kadaster.nl|*.SO.kadaster.nl</https--nonProxyHosts>
																		<geonetwork--dir>/nfs_dply/pdok/ngr3</geonetwork--dir>
                                    <reloadable>false</reloadable>
                                </map>
                            </properties>
                        </deployable>
						<deployable>
							<name>geonetwork-war</name>
							<contextRoot>geonetwork</contextRoot>
							<type>tomcat.War</type>
							<location>../web/target/geonetwork.war</location>
