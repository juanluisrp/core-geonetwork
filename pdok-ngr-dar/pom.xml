						</deployable>
						<deployable>
							<name>logging</name>
							<type>tomcat.LoggingSpec</type>
							<logging>
								<map>
									<DEFAULT-FATAL>{{logging.default-FATAL}}</DEFAULT-FATAL>
									<IGNORE-FATAL>{{logging.ignore-FATAL}}</IGNORE-FATAL>
									<DEFAULT-ERROR>{{logging.default-ERROR}}</DEFAULT-ERROR>
									<IGNORE-ERROR>{{logging.ignore-ERROR}}</IGNORE-ERROR>
									<DEFAULT-WARN>{{logging.default-WARN}}</DEFAULT-WARN>
									<IGNORE-WARN>{{logging.ignore-WARN}}</IGNORE-WARN>
									<DEFAULT-INFO>{{logging.default-INFO}}</DEFAULT-INFO>
									<IGNORE-INFO>{{logging.ignore-INFO}}</IGNORE-INFO>
									<DEFAULT-DEBUG>{{logging.default-DEBUG}}</DEFAULT-DEBUG>
									<IGNORE-DEBUG>{{logging.ignore-DEBUG}}</IGNORE-DEBUG>
									<DEFAULT-TRACE>{{logging.default-TRACE}}</DEFAULT-TRACE>
									<IGNORE-TRACE>{{logging.ignore-TRACE}}</IGNORE-TRACE>
								</map>
							</logging>
						</deployable>
						<deployable>
                            <name>nfs</name>
                            <type>tomcat.NfsSpec</type>
                            <NFS>
                                <map>
                                    <pdok>{{pdok.nfs.hostdir}}</pdok>
                                </map>
                            </NFS>
                        </deployable>
					</deployables>
				</configuration>
			</plugin>
		</plugins>
	</build>
	<properties>
		<deployit.username>jap-pdok-ont</deployit.username>
